{
  "name": "github-webhooks",
  "version": "0.0.0",
  "engines": {
    "node": ">=8.0.0 <=11.3.0",
    "yarn": "^1.0.0"
  },
  "main": "dist/server/index.js",
  "scripts": {
    "lint": "ng lint",
    "build:client": "ng build --prod",
    "build:server": "tsc -p tsconfig.server.json",
    "dev:client": "ng serve --open",
    "dev:server": "nodemon",
    "dev": "concurrently -n client,server \"yarn dev:client\" \"yarn dev:server\"",
    "build": "concurrently -n client,server \"yarn build:client\" \"yarn build:server\"",
    "format": "prettier --write 'src/**/*.{ts,scss,html}'",
    "postinstall": "if [ \"$NODE_ENV\" = production ]; then yarn build; fi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.2",
    "@angular/cdk": "^7.1.1",
    "@angular/cli": "~7.1.2",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bluebird": "^3.5.3",
    "core-js": "^2.5.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.3.15",
    "octicons": "^8.2.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "typescript": "~3.1.6",
    "winston": "^3.1.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/language-service": "~7.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0"
  }
}
